{
    "configurations": [
        {
            "name": "yaul",
            "includePath": [
                "${workspaceFolder}/**",
                "/opt/tool-chains/sh2eb-elf/sh2eb-elf/include/",
                "/opt/tool-chains/sh2eb-elf/sh2eb-elf/include/yaul/",
                "/opt/tool-chains/sh2eb-elf/sh2eb-elf/include/yaul/**"
            ],
            "defines": [
                "FENRIR_LOADER",
                "UNIT_TEST"
            ],
            "compilerPath": "/opt/tool-chains/sh2eb-elf/bin/sh2eb-elf-gcc",
            "compilerArgs": [
                "-ffreestanding",
                "-m2",
                "-mb"
            ],
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64",
            "compileCommands": "${workspaceFolder}/buildcmake/compile_commands.json",
            "configurationProvider": "ms-vscode.makefile-tools"
        },
        {
            "name": "linux",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "UNIT_TEST"
            ],
            "compilerPath": "/usr/sbin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64"
        },
        {
            "name": "mikmod",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/mikmod/libmikmod/include"
            ],
            "defines": [
                "DRV_SCSP_M68K=1"
            ],
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerPath": "/usr/bin/m68k-linux-gnu-gcc-13",
            "compilerArgs": []
        }
    ],
    "version": 4
}